   "source": [
    "#11. Model Evaluation\n",
    "\n",
    "# Calculate Accuracy (proportion of correct predictions)\n",
    "accuracy = accuracy_score(Y_test, y_pred)\n",
    "print(\"Accuracy:\", accuracy)\n",
    "\n",
    "# Calculate Precision (proportion of predicted Diagnosis=1 that are correct)\n",
    "# for binary\n",
    "#precision = precision_score(Y_test, y_pred)\n",
    "# for multi class\n",
    "precision = precision_score(Y_test, y_pred, average=\"weighted\")\n",
    "print(\"Precision:\", precision)\n",
    "\n",
    "# Calculate Recall (proportion of actual Diagnosis=1 correctly predicted)\n",
    "#for binary target\n",
    "#recall = recall_score(Y_test, y_pred)\n",
    "# for multi class target\n",
    "recall = recall_score(Y_test, y_pred, average=\"weighted\")\n",
    "print(\"Recall:\", recall)\n",
    "\n",
    "# Calculate F1-Score (harmonic mean of precision and recall)\n",
    "#for binary\n",
    "#f1 = f1_score(Y_test, y_pred)\n",
    "# for multi class target\n",
    "f1 = f1_score(Y_test, y_pred, average=\"weighted\")\n",
    "print(\"F1-Score:\", f1)\n",
    "\n",
    "# Calculate and print Confusion Matrix to show true positives, true negatives, false positives, false negatives\n",
